<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="150.5dp">

   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="150.5dp"
       >
    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="150.5dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/big_btn_on_selling"
        style="?android:attr/borderlessButtonStyle"
        />
   </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="129.5dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        >

        <ImageButton
            android:id="@+id/buttonOnSold"
            android:layout_width="298.5dp"
            android:layout_height="129.5dp"
            android:layout_marginLeft="30.8dp"
            android:layout_marginRight="30.8dp"
            android:layout_centerHorizontal="true"
            android:background="@drawable/rounded_rectangle1"
          />

        <ImageView
            android:id="@+id/registeredImg"
            android:layout_width="95.8dp"
            android:layout_height="95.8dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16.3dp"
            android:layout_marginRight="248dp"
            android:layout_marginStart="18dp"
            android:background="@drawable/img_on_selling"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintHorizontal_bias="0.029"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView4" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24.5dp"
            android:layout_marginLeft="128.3dp"
            android:layout_marginRight="44.3dp"
            android:textSize="13.5sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/modelName"
                android:layout_marginRight="2dp"
                android:text="서울" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="129.3dp"
            android:layout_marginTop="48.3dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/registeredPrice"
                android:text="3,000"
                android:textSize="20.3sp"/>

        </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:orientation="horizontal"
        android:layout_marginRight="48dp"
        android:layout_marginLeft="177.8dp"
        android:layout_marginTop="89.5dp">


        <Button
            android:id="@+id/reRegist"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="65dp"
            android:layout_height="25dp"
            android:layout_marginRight="6.5dp"
            android:background="@drawable/small_midium_radius_light_gray"
            android:text="재등록"
            android:textColor="@color/colorOrange"
            android:textSize="11.5sp" />

        <Button
            android:id="@+id/delete"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="45.4dp"
            android:layout_height="25dp"
            android:background="@drawable/small_medium_radius_bright_orange"
            android:text="삭제"
            android:textColor="#ffffff"
            android:textSize="11.5sp" />
    </LinearLayout>


    </RelativeLayout>


</RelativeLayout>